        <script>
          $('#search_bar').autocomplete({
            source: function(request, response) {
              if ($("#section").val() == "marketplace" || $("#section").val() == "downloads") {
                if (isAdvancedSearch()) {
                  $("#autocomplete_unavailable").show();
                } else {
                  setSearchBarFilterInfoText();
                  $.getJSON("/items/autocomplete_item_title", { 
                    term: $("#search_bar").val(), 
                    section: $("#section").val(),
                    software: $("#software").val(),
                    category: category_select.val(),
                    user: $("#search_bar_user").val()
                  }, response);
                  $("#autocomplete_unavailable").hide();
                }
              }
            },
            html: true,
            delay: 0,
            select: function(event, ui) { window.location.href = "/downloads/" + ui.item.id; event.preventDefault(); return false }
          });
